    "react-detect-click-outside": "^1.1.7",
    "react-dom": "^17.0.2",
    "react-ga4": "^2.1.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.5.0",
    "react-intl": "^6.8.9",
    "react-modal-video": "^1.2.10",
    "react-notifications-component": "^3.4.1",
    "react-qr-code": "^2.0.15",
    "react-router-dom": "^5.3.0",
    "react-select": "^5.10.1",
    "sass": "^1.85.1",
