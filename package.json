{
  "name": "company-website-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node server/server",
    "build": "npm run build:client && npm run build:server && npm run serve",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.jsx --outDir dist/server",
    "generate": "vite build --outDir dist/static && npm run build:server && node prerender",
    "serve": "cross-env NODE_ENV=production node server/server",
    "debug": "node --inspect-brk server"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^17.0.83",
    "@types/react-dom": "^17.0.26",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^2.2.16",
    "vite": "^6.1.0"
  },
  "dependencies": {
    "@google/model-viewer": "^1.12.1",
    "aos": "^2.3.4",
    "axios": "^0.24.0",
    "axios-cancel": "^0.2.2",
    "canvas": "^3.1.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "fs-extra": "^11.3.0",
    "path": "^0.12.7",
